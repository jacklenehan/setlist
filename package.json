{
  "name": "setlist",
  "version": "0.9.0",
  "description": "Setlist organizer.",
  "main": "index.js",
  "repository": "https://www.github.com/jacklenehan/setlist",
  "homepage": "http://www.jacklenehan.io/setlist",
  "author": "Jack Lenehan",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "autoprefixer": "9.1.5",
    "axios": "^0.18.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "coveralls": "^3.0.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "fetch-mock": "^6.0.1",
    "file-loader": "2.0.0",
    "file-saver": "^1.3.3",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "3.2.0",
    "immutability-helper": "^2.6.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "23.6.0",
    "lodash": "^4.17.5",
    "moment": "^2.19.3",
    "moment-duration-format": "^2.2.2",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.23.0",
    "supertest": "^3.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "start": "concurrently 'yarn start-client' 'yarn start-server'",
    "start-client": "node scripts/client.js",
    "start-server": "nodemon src/server/server.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.{js,jsx,mjs}",
      "src/server/**/*.{js,jsx,mjs}"
    ],
    "coverageDirectory": "./coverage",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
